  <%= form_with model: note, local: true do |f| %>
      <div class="form-group mt-3 mb-3 font-sarif-iro">
        <%= f.label '【音声を登録できます】' %><br>
      </div>
      <div class="form-group">
        <%= f.file_field :audio, :size => 140 %>
      </div>
      <div class="form-group mt-3 mb-3 font-sarif-iro">
        <%= f.label '【曲名】' %><br>
        <%= f.text_field :song_title %>
      </div>
      <div class="form-group mt-3 mb-3 font-sarif-iro">
        <%= f.label '【歌手名】' %><br>
        <%= f.text_field :singer_name %>
      </div>
      <div class="form-group mt-3 mb-3 font-sarif-iro">
        <%= f.label '【ハッシュタグ】' %><br>
        <%= f.text_field :hashbody %>
      </div>
      <div class="form-group m-sm-3 font-sarif-iro">
        <%= f.label '【振り譜】' %><br>
        <%= f.rich_text_area :content %>
      </div>
      <div class="form-group mt-3 mb-3">
        <%= f.submit class: 'btn btn-outline-primary' %>
      </div>
  <% end %>
